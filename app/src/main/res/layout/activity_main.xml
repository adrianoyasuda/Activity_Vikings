<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">


    <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

            <ImageView
                    android:layout_width="105dp"
                    android:layout_height="106dp" app:srcCompat="@drawable/floki"
                    android:id="@+id/img_Floki" android:layout_marginTop="8dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView" android:layout_marginLeft="16dp"
                    android:layout_marginStart="16dp"/>
            <ImageView
                    android:layout_width="105dp"
                    android:layout_height="106dp" app:srcCompat="@drawable/rollo"
                    android:id="@+id/img_Rollo" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
                    android:layout_marginRight="8dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginBottom="8dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintVertical_bias="0.153"
                    app:layout_constraintTop_toBottomOf="@+id/img_imagem"
            />
            <TextView
                    android:text="Ragnar Lothbrok"
                    android:layout_width="107dp"
                    android:layout_height="22dp"
                    android:id="@+id/textView"
                    android:layout_marginTop="8dp"
                    app:layout_constraintTop_toBottomOf="@+id/img_Ragnar"
                    app:layout_constraintEnd_toEndOf="@+id/img_Ragnar"
                    app:layout_constraintStart_toStartOf="@+id/img_Ragnar"/>
            <TextView
                    android:text="Rollo"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView3" android:layout_marginTop="8dp"
                    app:layout_constraintTop_toBottomOf="@+id/img_Rollo"
                    app:layout_constraintEnd_toEndOf="@+id/img_Rollo"
                    app:layout_constraintStart_toStartOf="@+id/img_Rollo"/>
            <ImageView
                    android:layout_width="105dp"
                    android:layout_height="106dp" app:srcCompat="@drawable/bjorn"
                    android:id="@+id/img_Bjorn"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginTop="12dp"
                    app:layout_constraintTop_toBottomOf="@+id/textView3"
                    android:layout_marginEnd="8dp"
                    android:layout_marginRight="8dp"/>
            <ImageView
                    android:layout_width="376dp"
                    android:layout_height="73dp"
                    app:srcCompat="@drawable/logo"
                    android:id="@+id/img_imagem"
                    android:layout_marginEnd="8dp"
                    android:layout_marginRight="8dp" app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginLeft="8dp"
                    android:layout_marginStart="8dp"
                    android:layout_marginBottom="8dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:layout_marginTop="8dp"
                    app:layout_constraintVertical_bias="0.0"
                    app:layout_constraintTop_toTopOf="parent"
                    android:contentDescription="TODO" app:layout_constraintHorizontal_bias="0.5"/>
            <TextView
                    android:text="Bjorn Ironside"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView6"
                    android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/img_Bjorn"
                    app:layout_constraintEnd_toEndOf="@+id/img_Bjorn"
                    app:layout_constraintStart_toStartOf="@+id/img_Bjorn"/>
            <TextView
                    android:text="Lagertha"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView2" android:layout_marginTop="8dp"
                    app:layout_constraintTop_toBottomOf="@+id/img_Lagertha"
                    app:layout_constraintStart_toStartOf="@+id/img_Lagertha"
                    app:layout_constraintEnd_toEndOf="@+id/img_Lagertha"/>
            <TextView
                    android:text="Floki"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView4"
                    android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/img_Floki"
                    app:layout_constraintStart_toStartOf="@+id/img_Floki"
                    app:layout_constraintEnd_toEndOf="@+id/img_Floki"/>
            <TextView
                    android:text="Athelstan"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView5"
                    app:layout_constraintStart_toStartOf="@+id/img_Athelstan"
                    app:layout_constraintEnd_toEndOf="@+id/img_Athelstan" android:layout_marginTop="8dp"
                    app:layout_constraintTop_toBottomOf="@+id/img_Athelstan"/>
            <ImageView
                    android:layout_width="105dp"
                    android:layout_height="106dp" app:srcCompat="@drawable/ragnar"
                    android:id="@+id/img_Ragnar" android:layout_marginTop="8dp"
                    app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginLeft="16dp" android:layout_marginStart="16dp" android:layout_marginBottom="8dp"
                    app:layout_constraintVertical_bias="0.153"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/img_imagem"/>
            <ImageView
                    android:layout_width="105dp"
                    android:layout_height="106dp" app:srcCompat="@drawable/lagertha"
                    android:id="@+id/img_Lagertha"
                    android:layout_marginLeft="8dp"
                    android:layout_marginStart="8dp" android:layout_marginTop="8dp"
                    android:layout_marginBottom="8dp"
                    app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.153"
                    android:layout_marginEnd="8dp"
                    android:layout_marginRight="8dp"
                    app:layout_constraintTop_toBottomOf="@+id/img_imagem"
                    app:layout_constraintStart_toEndOf="@+id/img_Ragnar"
                    app:layout_constraintEnd_toStartOf="@+id/img_Rollo"/>
            <ImageView
                    android:layout_width="105dp"
                    android:layout_height="106dp" app:srcCompat="@drawable/athelstan"
                    android:id="@+id/img_Athelstan" android:layout_marginTop="12dp"
                    android:layout_marginStart="8dp"
                    android:layout_marginLeft="8dp" android:layout_marginEnd="8dp"
                    android:layout_marginRight="8dp"
                    app:layout_constraintTop_toBottomOf="@+id/textView2"
                    app:layout_constraintStart_toEndOf="@+id/img_Floki"
                    app:layout_constraintEnd_toStartOf="@+id/img_Bjorn"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>